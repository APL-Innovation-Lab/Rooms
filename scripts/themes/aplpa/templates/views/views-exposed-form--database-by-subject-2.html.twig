{#
/**
 * @file
 * Default theme implementation of a views exposed form.
 *
 * Available variables:
 * - form: A render element representing the form.
 *
 * @see template_preprocess_views_exposed_form()
 *
 * @ingroup themeable
 */
#}
{% if q is not empty %}
  {#
    This ensures that, if clean URLs are off, the 'q' is added first,
    as a hidden form element, so that it shows up first in the POST URL.
  #}
{{ q }}
{% endif %}
  <div class="grid-offset-1 grid-col-10 usa-accordion">
    <button type="button" class="usa-accordion__button" aria-expanded="true" aria-controls="f-1">Filter</button>

    <div id="f-1" class="details usa-accordion__content usa-prose">
{{ form }}
    </div>
  </div>
